plugins {
	id 'nf-ga4gh'
}
process {
	executor = 'tes'
	container = 'quay.io/cdis/gen3-workflow:integration_tests_dicom_image'
}
tes {
	endpoint = "https://${HOSTNAME}/ga4gh/tes"
	oauthToken = "${GEN3_TOKEN}"
}
aws {
	accessKey = "${GEN3_TOKEN}"
	secretKey = 'N/A'
	region = 'us-east-1'
	client {
		endpoint = "https://${HOSTNAME}/workflows/s3"
		s3PathStyleAccess = true
		maxErrorRetry = 1
    }
}
workDir = "${WORK_DIR}"
